{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
        "logo": {
        "type": "auto",
        "source": "/var/home/mark/.config/fastfetch/bluefin.png",
        "padding": {
            "top": 1
        }
    },
    "display": {
        "separator": "->   "
    },
    "modules": [
        {
            "type": "custom",
            "format": "┌───────────────────────────── \u001b[1mSystem Information\u001b[0m ─────────────────────────────┐" // `\u001b` is `\033`, or `\e`
        },
        "break",
            {
      	      "type": "os",
      	      "key": "                    ",
      	      "format": "{pretty-name}     "
        },   
        {
            "key": "     Kernel         ",
            "keyColor": "magenta",
            "type": "kernel"
        },
        {
            "key": "     WM             ",
            "keyColor": "green",
            "type": "wm"
        },        
        {
            "key": "     Terminal       ",
            "type": "terminal",
        },
        {
            "key": "    󰻠 CPU            ",
            "keyColor": "yellow",
            "type": "cpu"
        },
        {
            "key": "    󰍛 GPU            ",
            "keyColor": "blue",
            "type": "gpu"
        },
        {
            "key": "    󰑭 Memory         ",
            "keyColor": "magenta",
            "type": "memory"
        },
         {
            "key": "     Packages       ",
            "keyColor": "magenta",
            "type": "packages"
        },
        "break",
        {
            "type": "custom",
            "key": " ",
            "format": "└──────────────────────────────────────────────────────────────────────────────┘" // `\u001b` is `\033`, or `\e`
        },
           
